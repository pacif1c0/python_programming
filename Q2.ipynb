    "# Q2 Answer template\n",
    "\n",
    "N = int(input())\n",
    "count = 0\n",
    "curr_val = N\n",
    "\n",
    "while True:\n",
    "    new_val = ((curr_val % 10) * 10) + ((curr_val // 10 + curr_val % 10) % 10)\n",
    "    count += 1\n",
    "    \n",
    "    if new_val == N:\n",
    "        break\n",
    "    else:\n",
    "        curr_val = new_val\n",
    "    \n",
    "print(count)"
   ]
